var product_cards = []
<% @product_cards.each do |product| %>
  product_cards.push("<%= j render 'spree/shared/standard_product_card', product: product %>");
<% end %>

var sliderCards = $('#best_seller_products').find($('.slider-card[data-tab="<%= params[:tab] %>"]'));

sliderCards.each(function(index){
  $(this).html(product_cards[index]).data('loaded', true);
});

