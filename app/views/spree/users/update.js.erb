<% if @user.errors.present? %>
  $('#email-message').html('<%= @user.errors[:email].join(', ').prepend('Error: Email ') =%>');
  $('#email').css('border-color', '#E32754');
  $('#email-icon').show().addClass('fas fa-exclamation-circle').removeClass('fa-eye fa-check-circle').css('color','#E32754');

<% else %>
  $('#editUser').hide();
  $('.modal-backdrop').hide();
  $("[data-dismiss=modal]").trigger({ type: "click" });
  location.reload();

<% end %>

