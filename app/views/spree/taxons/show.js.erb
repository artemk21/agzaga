var data = '';

<% @products.each do |product| %>
  data += '<%= j render(partial: "spree/shared/product_card", locals: { product: product }) %>';
<% end %>
$('#products').html(data);

$('#indiviual-section').html('<%= j (paginate(@products, theme: 'ag_pagination', left: 3, right: 1, window: 1)) %>');

