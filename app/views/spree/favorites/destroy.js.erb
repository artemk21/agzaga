<% if @favorite.destroyed? %>
  if ($('.orders.edit').length == 0)
  {
    var $el = $(".favorable_<%= @favorite.favorable_type.demodulize.underscore %>_<%= @favorite.favorable_id %>");

    if ($el) {
      $el.each(function( index, element ) {
        element.href = '<%= favorites_path(favorable_id: @favorite.favorable_id, favorable_type: @favorite.favorable_type) %>';
        element.dataset.method = 'post';
        $('#slider-adc-form').html('<%= j render 'spree/shared/slider_v2'%>');
      });
    } else {
      $(".favorite_<%= @favorite.id %>").remove();
    }
  }
  else
  {
    location.reload();
  }
<% end %>

