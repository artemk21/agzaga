<% products ||= nil %>
<% heading_text ||= "Products" %>

<div class="data-container featured-carousel">
  <h2 class="section-heading"><%= heading_text %></h2>

  <div class="container-fluid p-0">
    <div id="addition-carousel" class="owl-carousel owl-theme home-page-slider">
      <% products.each do |product| %>
        <div class="item">
          <a href="<%= product_path(product) %>" class="clear-link" data-turbolinks="false">
            <%= render partial: 'spree/shared/standard_product_card', locals: { product: product } %>
          </a>
        </div>
      <% end %>
    </div>
  </div>
</div>

